let tpl = import "std/tuples.ucg";
let t = (import "std/testing.ucg").asserts{};

assert t.equal{
    left = tpl.fields{tpl={foo=1, bar=2}}.result,
    right = ["foo", "bar"],
};

assert t.equal{
    left = tpl.values{tpl={foo=1, bar=2}}.result,
    right = [1, 2],
};

assert t.equal{
    left = tpl.enumerate{tpl={foo=1, bar=2}}.result,
    right = [["foo", 1], ["bar", 2]],
};